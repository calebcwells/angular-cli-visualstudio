{
  "project": {
    "version": "1.0.0-beta.24",
    "name": "angular2cli"
  },
  "apps": [
    {
      "root": "Client",
      "assets": [
        "favicon.ico"
      ],
      "main": "main.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "ng2cli",
      "mobile": false,
      "styles": [
        "styles.css"
      ]
